  "type": "module",
  "main": "index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "vitest",
    "test:coverage": "vitest --coverage --run",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix",
    "db:setup": "cd ../ && docker compose up -d",
    "db:down": "cd ../ && docker compose down",
    "db:migrate": "dotenv -e .env -- prisma migrate dev",
    "db:reset": "dotenv -e .env -- prisma migrate reset"
  },
  "dependencies": {
    "@prisma/client": "^6.11.1",
    "express": "^4.18.2",
    "socket.io": "^4.8.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.16.0",
    "@vitest/coverage-v8": "^3.2.4",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9.16.0",
    "globals": "^15.13.0",
    "nodemon": "^3.0.2",
    "socket.io-client": "^4.8.1",
    "supertest": "^7.1.3",
    "vitest": "^3.2.4"
  }
}
