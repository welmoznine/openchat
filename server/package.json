{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "license": "ISC",
  "author": "",
  "type": "module",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "main": "index.js",
  "prisma": {
    "seed": "node prisma/seed.js"
  },
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "vitest",
    "test:coverage": "vitest --coverage --run",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix",
    "db:setup": "cd ../ && docker compose up -d",
    "db:down": "cd ../ && docker compose down",
    "db:migrate": "dotenv -e .env -- prisma migrate dev",
    "db:reset": "dotenv -e .env -- prisma migrate reset",
    "db:seed": "prisma db seed",
    "db:ui": "dotenv -e .env -- prisma studio",
    "db:test:setup": "cd ../ && docker compose up -d && npm run db:test:migrate",
    "db:test:migrate": "dotenv -e .env.test -- prisma migrate deploy",
    "db:test:reset": "dotenv -e .env.test -- prisma migrate reset --force",
    "db:test:seed": "dotenv -e .env.test -- prisma db seed"
  },
  "dependencies": {
    "@prisma/client": "^6.11.1",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "debug": "^4.4.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "socket.io": "^4.8.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.16.0",
    "@vitest/coverage-v8": "^3.2.4",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9.16.0",
    "globals": "^15.13.0",
    "nodemon": "^3.0.2",
    "prisma": "^6.11.1",
    "socket.io-client": "^4.8.1",
    "supertest": "^7.1.3",
    "vitest": "^3.2.4"
  }
}
